<h2>New Gallery</h2>
<% form_for(@gallery, :as => :gallery, :url => grandstand_galleries_path) do |form| -%>
  <%= render :partial => 'form', :locals => {:form => form} %>
  <div class="toolbar">
    <%= button 'Add Gallery' %>
    <%= button_link_to('Cancel', grandstand_galleries_path, :class => 'cancel') %>
  </div>
<% end -%>
